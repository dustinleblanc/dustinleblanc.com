        "cross-env": "^5.1",
        "laravel-mix": "^4.0",
        "laravel-mix-purgecss": "^4.0.0",
        "postcss-import": "^12.0.1",
        "postcss-nested": "^4.1.2",
        "postcss-preset-env": "^6.7.0",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@tailwindcss/aspect-ratio": "^0.2.0",
